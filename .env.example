# M-TEAM 用户名
MT_USERNAME=your_username_here

# M-TEAM 密码
MT_PASSWORD=your_password_here

# Telegram Bot Token (向 @BotFather 获取)
TG_BOT_TOKEN=123456:ABC-DEF1234ghIkl-zyx57W2v1u123ew11

# Telegram User ID (向 @userinfobot 获取)
TG_USER_ID=123456789

# 定时任务表达式 (默认每天上午 9 点)
# 格式: 秒 分 时 日 月 周
CRON_EXPRESSION=0 9 * * *

# 随机延迟最大时间 (毫秒)，默认 45 分钟 (2700000)
RANDOM_DELAY_MAX=2700000

# 本地测试时是否跳过随机延迟 (true/false)
SKIP_DELAY=false

# 启动容器时是否立即执行一次 (true/false)
# 调试模式建议开启，方便确认配置是否正确
RUN_ON_START=true
